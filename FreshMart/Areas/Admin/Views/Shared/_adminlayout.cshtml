<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>FreshMart-Admin</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link href="~/Assets/Admin/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Assets/Admin/css/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Ionicons -->
    @*<link href="~/Assets/Admin/css/ionicons.min.css" rel="stylesheet" />*@
    <!-- jvectormap -->
    <link href="~/Assets/Admin/css/jquery-jvectormap.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/Assets/Admin/css/AdminLTE.min.css" rel="stylesheet" />
    <!-- AdminLTE Skins. Choose a skin from the css/skins
       folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/AdminComponents/dist/css/skins/_all-skins.min.css">

    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>

<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        <header class="main-header">

            <!-- Logo -->
            <a href="/admin" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>A</b>LT</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Freshmart Admin</b></span>
            </a>

            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Nav</span>
                </a>

            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">

                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">MAIN NAVIGATION</li>
                    <li class="active treeview menu-open">
                        <a href="#">
                            <i class="fa fa-dashboard text-blue"></i> <span>Home</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/admin"><i class="fa fa-circle-o text-aqua"></i> Admin</a></li>
                            <li class="active">
                                <a href="/"> <i class="fa fa-circle-o"></i>Website</a>

                            </li>
                        </ul>
                    </li>

                    @* product manage *@
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-product-hunt"></i>
                            <span>Product Manager</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/admin/productmanager"> <i class="fa fa-circle-o text-aqua"></i> All Products</a></li>
                        </ul>
                    </li>

                    @* order manage *@
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-product-hunt"></i>
                            <span>Order Manager</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/admin/ordermanager"> <i class="fa fa-circle-o text-blue"></i> All Orders</a></li>
                        </ul>
                    </li>

                    @* order manage *@
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-product-hunt text-aqua"></i>
                            <span>Buy-Sell Manager</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/admin/SellerBuyerManager/RequestIndex"> <i class="fa fa-circle-o text-yellow"></i>Seller requests </a></li>
                            <li><a href="/admin/SellerBuyerManager/SellerIndex"> <i class="fa fa-circle-o text-aqua"></i>All Sellers </a></li>
                            <li><a href="/admin/SellerBuyerManager/CustomerIndex"> <i class="fa fa-circle-o text-blue"></i>All Customers </a></li>

                        </ul>
                    </li>

                    @* categories manage *@
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-product-hunt"></i>
                            <span>Category Manager</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/admin/Categories"> <i class="fa fa-circle-o text-yellow"></i>All Categories </a></li>
                            <li><a href="/admin/Categories/create"> <i class="fa fa-circle-o text-aqua"></i>Add Categories </a></li>
                            <li><a href="/admin/CategoryDomains"> <i class="fa fa-circle-o text-blue"></i>All Category Domains</a></li>
                            <li><a href="/admin/CategoryDomains/create"> <i class="fa fa-circle-o text-green"></i>Add Category Domains </a></li>

                        </ul>
                    </li>

                    @* agent manage *@
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-product-hunt"></i>
                            <span>Agent Manager</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/admin/Agents"> <i class="fa fa-circle-o text-yellow"></i>All Agents </a></li>
                            @*<li><a href="/admin/Agents/create"> <i class="fa fa-circle-o text-aqua"></i>Add Agent </a></li>*@
                        </ul>
                    </li>


                    @* Districts manage *@
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-product-hunt"></i>
                            <span>District Manager</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/admin/DistrictManager"> <i class="fa fa-circle-o text-yellow"></i>All Districts </a></li>
                            <li><a href="/admin/DistrictManager/create"> <i class="fa fa-circle-o text-aqua"></i>Add District </a></li>

                        </ul>
                    </li>



                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>



        @RenderBody()




        <!-- Add the sidebar's background. This div must be placed
        immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>

    </div>
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- FastClick -->

    <script src="~/Assets/Admin/js/fastclick.js"></script>
    <!-- AdminLTE App -->
    <!-- Sparkline -->
    <script src="~/Assets/Admin/js/adminlte.min.js"></script>

    @*<script src="~/Assets/Admin/js/jquery.sparkline.min.js"></script>*@

    <!-- jvectormap  -->
    @*<script src="~/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>*@
    <!-- SlimScroll -->
    <script src="~/Assets/Admin/js/jquery.slimscroll.min.js"></script>
    <!-- ChartJS -->
    <script src="~/Assets/Admin/js/Chart.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    @*<script src="~/Assets/Admin/js/dashboard2.js"></script>*@
    <!-- AdminLTE for demo purposes -->
    <script src="~/Assets/Admin/js/demo.js"></script>
</body>

</html>